# Force Unix line-endings
*.json eol=lf
*.lock eol=lf
*.md eol=lf
*.php eol=lf
*.xml eol=lf

# Remove all test and documentation files from releases with 'export-ignore'.
/Tests              export-ignore
/.gitattributes     export-ignore
/.gitignore         export-ignore
/.travis.yml        export-ignore
/phpunit.xml.dist   export-ignore
